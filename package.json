{
  "name": "@ligalaiz/registration-form",
  "version": "1.0.0",
  "description": "pet project",
  "private": true,
  "scripts": {
    "build": "webpack --config config/webpack.prod.js",
    "start:client": "webpack serve --config config/webpack.dev.js",
    "lint": "eslint ./src --cache",
    "lint:fix": "eslint ./src  --fix",
    "clean": "rm -rf dist",
    "test": "jest --forceExit",
    "test:smoke": "jest --forceExit --t=#smoke",
    "test:coverage": "jest --forceExit --coverage",
    "test:mutation": "npx stryker run",
    "loki:approve": "npx loki approve",
    "loki:update": "npm run build-storybook && npx loki update --requireReference --reactUri file:./storybook-static",
    "test:loki": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static",
    "check": "npm run lint && npm test && npm run test:loki",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "storybook:withCantGetError": "npx start-storybook --no-manager-cache",
    "cypress:open": "cypress open",
    "cypress:run": "npx cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ligalaiz/registration-form.git"
  },
  "keywords": [
    "react",
    "components"
  ],
  "author": "Ligalaiz",
  "license": "MIT",
  "bugs": {
